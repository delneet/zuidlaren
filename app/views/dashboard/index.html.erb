<h1>Dashboard</h1>
<div class="row">
  <div class="col-md-6">
    <h2>Teams</h2>
    <table class="table">
      <tbody>
      <% @teams.each do |team| %>
          <tr>
            <td><%= link_to team.name, edit_team_path(team) %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
  <div class="col-md-6">
    <h2>Users</h2>
    <table class="table">
      <tbody>
      <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.name, edit_user_path(user) %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>