<%= simple_form_for @user do |f| %>
  <%= f.input :name %>
  <%= f.input :github_username %>
  <%= f.input :slack_username %>

  <h2>Teams</h2>
  <%= f.association :teams, collection: Team.all, as: :check_boxes, label: false, class: 'text' %>

  <%= f.submit class: "btn btn-primary" %>
  <%= link_to 'Back', users_path, class: "btn" %>
<% end %>